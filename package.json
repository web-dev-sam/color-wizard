{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "pre": "nuxt prepare",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "test": "vitest",
    "types": "nuxt typecheck",
    "lint": "eslint --cache .",
    "check": "pnpm run lint && pnpm run types",
    "postinstall": "nuxt prepare",
    "prepare": "husky && nuxt prepare",
    "clean": "rm -r .nuxt .output node_modules .eslintcache",
    "cn": "shadcn-vue add && rsync -a --remove-source-files ../components/ ./components/ && find ../components/ -depth -type d -empty -exec rmdir {} \\;"
  },
  "dependencies": {
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/icon": "^1.10.2",
    "@nuxt/image": "^1.8.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/seo": "^2.0.2",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@vueuse/core": "^12.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "culori": "^4.0.1",
    "limiter": "^2.1.0",
    "lucide-vue-next": "^0.468.0",
    "nuxt": "^3.14.1592",
    "nuxt-security": "^2.1.5",
    "radix-vue": "^1.9.11",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.5",
    "vue": "latest",
    "vue-router": "latest",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.12.0",
    "@iconify-json/carbon": "^1.2.5",
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/octicon": "^1.2.2",
    "@iconify-json/ph": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.16",
    "@iconify-json/tabler": "^1.2.13",
    "@tailwindcss/typography": "^0.5.15",
    "@types/culori": "^2.1.1",
    "@types/pg": "^8.11.10",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "shadcn-vue": "^0.11.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.6",
    "typescript-eslint": "^8.18.1",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  },
  "overrides": {
    "vite-plugin-checker": "0.8.0",
    "vue-tsc": "2.0.29"
  },
  "lint-staged": {
    "*.{js,ts,vue,json}": [
      "eslint"
    ]
  }
}
